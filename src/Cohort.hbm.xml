<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gc.dto.Cohort" table="Cohort"
		schema="SurveyDB">


		<id name="cohortID">	
			<column name="CohortID" sql-type="int(11)"></column>
		</id>

		<property name="cohortName">
			<column name="CohortName" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="cohortSemester">
			<column name="CohortSemester" sql-type="varchar(255)" length="255"></column>
		</property>
	</class>
</hibernate-mapping>		