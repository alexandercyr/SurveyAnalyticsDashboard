<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sad.dto.Survey" table="Survey"
		schema="SurveyDB">


		<id name="surveyID">	
			<column name="SurveyID" sql-type="int(11)"></column>
		</id>

		<property name="description">
			<column name="Description" sql-type="varchar(255)" length="255"></column>
		</property>
		<property name="frequency">
			<column name="Frequency" sql-type="date"></column>
		</property>
		<property name="createDate">
			<column name="CreateDate" sql-type="date"></column>
		</property>



	</class>
</hibernate-mapping>		